@model IEnumerable<MA.Models.UserInfo>
@{
    ViewData["Title"] = "Index";
}




<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                昵称
            </th>
            <th>
                用户名
            </th>
            <th>
                邮箱
            </th>
            <th></th>
        </tr>
        
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NickName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdentityUserId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdentityUserId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdentityUserId">Delete</a>
            </td>
        </tr>
        }
    </tbody>
</table>


